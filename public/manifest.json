{
    "name": "Salesforce Debug Logs (sfdl)",
    "description": "Increases productivity during debugging process Apex log analysis",
    "version": "1.4.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js",
        "mode": "module"
    },
    "host_permissions": [
        "https://*.salesforce.com/",
        "https://*.force.com/"
    ],
    "permissions": [
        "cookies", "storage"
    ],
    "icons": {
        "16": "/resources/sfdl16.png",
        "48": "/resources/sfdl48.png",
        "128": "/resources/sfdl128.png"
    }
}